<Page x:Class="WpfApp3.pages.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:WpfApp3.pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page2">

    <Grid>
        <StackPanel HorizontalAlignment="Center"
                    VerticalAlignment="Center">
            <TextBlock Text="Name"/>
            <TextBox x:Name="NamrTb" Width="100"/>
            <TextBlock Text="Date"/>
            <TextBox x:Name="DateTb"/>
            <Button Content="Generete"
                    x:Name="deneretBtn"
                    Click="deneretBtn_Click" Margin="5"/>
            <TextBlock Text="Login"/>
            <TextBox x:Name="LoginTb"/>
            <TextBlock Text="Password"/>
            <TextBox Name="PasswordTb"/>
        </StackPanel>
    </Grid>
</Page>
